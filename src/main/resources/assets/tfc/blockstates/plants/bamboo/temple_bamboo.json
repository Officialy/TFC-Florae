{
  "multipart": [
    {
      "when": {
        "part": "lower|middle"
      },
      "apply": [
        {
          "model": "tfc:plants/temple_bamboo_lower"
        }
      ]
    },
    {
      "when": {
        "part": "upper|single"
      },
      "apply": [
        {
          "model": "tfc:plants/temple_bamboo_upper"
        }
      ]
    },
    {
      "when": {
        "part": "upper|single"
      },
      "apply": [
        {
          "model": "tfc:plants/temple_bamboo_upper",
          "y": 180
        }
      ]
    }
  ]
}
