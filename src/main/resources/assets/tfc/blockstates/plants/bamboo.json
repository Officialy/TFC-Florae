{
  "multipart": [
    {
      "when": {
        "part": "single"
      },
      "apply": [
        {
          "model": "tfc:plants/arrow_bamboo_single"
        }
      ]
    },
    {
      "when": {
        "part": "lower"
      },
      "apply": [
        {
          "model": "tfc:plants/small_arrow_bamboo"
        }
      ]
    },
    {
      "when": {
        "part": "middle"
      },
      "apply": [
        {
          "model": "tfc:plants/small_arrow_bamboo",
          "weight": 2
        },
        {
          "model": "tfc:plants/small_arrow_bamboo_single_leaf",
          "weight": 2
        },
        {
          "model": "tfc:plants/small_arrow_bamboo_small_leaves",
          "weight": 1
        },
        {
          "model": "tfc:plants/small_arrow_bamboo_large_leaves",
          "weight": 1
        }
      ]
    },
    {
      "when": {
        "part": "upper"
      },
      "apply": [
        {
          "model": "tfc:plants/small_arrow_bamboo_top",
          "y": 180
        }
      ]
    }
  ]
}
