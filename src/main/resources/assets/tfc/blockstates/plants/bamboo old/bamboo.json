{
    "multipart": [
        {   "when": { "age": "0" },
            "apply": [
                { "model": "tfc:plants/bamboo1_age0" },
                { "model": "tfc:plants/bamboo2_age0" },
                { "model": "tfc:plants/bamboo3_age0" },
                { "model": "tfc:plants/bamboo4_age0" }
            ]
        },
        {   "when": { "age": "1" },
            "apply": [
                { "model": "tfc:plants/bamboo1_age1" },
                { "model": "tfc:plants/bamboo2_age1" },
                { "model": "tfc:plants/bamboo3_age1" },
                { "model": "tfc:plants/bamboo4_age1" }
            ]
        },
        {   "when": { "leaves": "small" },
            "apply": { "model": "tfc:plants/bamboo_small_leaves" }
        },
        {   "when": { "leaves": "large" },
            "apply": { "model": "tfc:plants/bamboo_large_leaves" }
        }
    ]
}
