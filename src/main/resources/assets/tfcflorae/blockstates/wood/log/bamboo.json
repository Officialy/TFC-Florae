      },
      "apply": [
		{
          "model": "tfcflorae:block/wood/log/bamboo/bamboo",
		  "weight": 1,
		  "x": 90,
		  "y": 90
		},
		{
          "model": "tfcflorae:block/wood/log/bamboo/bamboo_leaves",
